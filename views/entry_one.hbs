<div class="entry_one">
  <div class="mdl-grid content-grid vs vert-div">
    <div class="mdl-cell mdl-grid.center-items vert-div horz-div">
      <h4 class="interactive-title">How are you feeling today {{{user.profile.name.first}}}?</h4>
      <form method="POST" class="account-form interactive-form" action="/entry">
        <input type="hidden" name="email" value="{{{user.email}}}"/>
        <input type="hidden" name="name" value="{{{user.profile.name.first}}}"/>
        <input type="hidden" name="phone" value="{{{user.profile.phone}}}"/>
        <div class="card interactive-card">
          <label class="mdl-icon-toggle mdl-js-icon-toggle mdl-icon-toggle-interactive" for="feeling_good">
            <input type="checkbox" name="feeling_good" id="feeling_good" value="{{{entry.feeling.good}}}" class="mdl-icon-toggle__input">
            <i class="mdl-icon-toggle__label material-icons material-icons-interactive">sentiment_very_satisfied</i>
          </label>
          <h6 class="interactive-icon-title">Good</h6>
        </div>
        <div class="card interactive-card">
          <label class="mdl-icon-toggle mdl-js-icon-toggle mdl-icon-toggle-interactive" for="feeling_ok">
            <input type="checkbox" name="feeling_ok" id="feeling_ok" class="mdl-icon-toggle__input" value="{{{entry.feeling.ok}}}" >
            <i class="mdl-icon-toggle__label material-icons material-icons-interactive">sentiment_satisfied</i>
          </label>
          <h6 class="interactive-icon-title">Ok</h6>
        </div>
        <div class="card interactive-card">
          <label class="mdl-icon-toggle mdl-js-icon-toggle mdl-icon-toggle-interactive" for="feeling_strong">
            <input type="checkbox" name="feeling_strong" id="feeling_strong" class="mdl-icon-toggle__input" value="{{{entry.feeling.confident}}}">
            <i class="mdl-icon-toggle__label material-icons material-icons-interactive">fitness_center</i>
          </label>
          <h6 class="interactive-icon-title">Strong / Confident</h6>
        </div>
        <div class="card interactive-card">
          <label class="mdl-icon-toggle mdl-js-icon-toggle mdl-icon-toggle-interactive" for="feeling_moody">
            <input type="checkbox" id="feeling_moody" name="feeling_moody" class="mdl-icon-toggle__input" value="{{{entry.feeling.moody}}}" >
            <i class="mdl-icon-toggle__label material-icons material-icons-interactive">sentiment_neutral</i>
          </label>
          <h6 class="interactive-icon-title">Moody</h6>
        </div>
        <div class="card interactive-card">
          <label class="mdl-icon-toggle mdl-js-icon-toggle mdl-icon-toggle-interactive" for="feeling_sad">
            <input type="checkbox" id="feeling_sad" name="feeling_sad" class="mdl-icon-toggle__input" value="{{{entry.feeling.sad}}}" >
            <i class="mdl-icon-toggle__label material-icons material-icons-interactive">sentiment_very_dissatisfied</i>
          </label>
          <h6 class="interactive-icon-title">Sad</h6>
        </div>
        <div class="card interactive-card">
          <label class="mdl-icon-toggle mdl-js-icon-toggle mdl-icon-toggle-interactive" for="feeling_anxious">
            <input type="checkbox" id="feeling_anxious" name="feeling_anxious" class="mdl-icon-toggle__input" value="{{{entry.feeling.anxious}}}">
            <i class="mdl-icon-toggle__label material-icons material-icons-interactive">mood_bad</i>
          </label>
          <h6 class="interactive-icon-title">Anxious</h6>
        </div></br>
      </form>
    </div>
  <div class="interactive-footer">
    <a href="/entry">
      <button class="mdl-button mdl-js-button mdl-button--accent mdl-button--white interactive-button-back">Back</button>
    </a>
    <button type="submit" class="mdl-button mdl-js-button mdl-button--accent mdl-button--white interactive-button">Next</button>
  </div>
</div>
